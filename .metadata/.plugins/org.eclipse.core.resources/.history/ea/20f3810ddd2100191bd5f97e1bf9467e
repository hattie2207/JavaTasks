package PaintWizard;

public class Paint {
	
	private String paintBrand;
	private int volume;
	private double price, coverSpace;
	
	public Paint(String paintBrand, int volume, double price, double coverSpace) {
		
		this.paintBrand = paintBrand;
		this.volume = volume;
		this.price = price;
		this.coverSpace = coverSpace;
		
	}	
	
	public String getPaintBrand() {
		return paintBrand;
	}



	public void setPaintBrand(String paintBrand) {
		this.paintBrand = paintBrand;
	}



	public int getVolume() {
		return volume;
	}



	public void setVolume(int volume) {
		this.volume = volume;
	}



	public double getPrice() {
		return price;
	}



	public void setPrice(double price) {
		this.price = price;
	}



	public double getCoverSpace() {
		return coverSpace;
	}



	public void setCoverSpace(double coverSpace) {
		this.coverSpace = coverSpace;
	}



	public String toString() {
		return "";
	}

}
